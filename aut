                 <h2 class="default">
                    Product description
                    
                 </h2>
                
                
                

            
            
                
                
                  





    <div id="productDescription" class="a-section a-spacing-small">
        





  
    
  


        
        
     
	       
     

     
                              
     
       	       
        
        <!-- show up to 2 reviews by default --> 
        
        	
        	
        		
        			<p>L.O.L. Surprise! Pearl Surprise
				    	
					</p>
        		
        		
        	
        
                
        
        
      
      
    
    </div>

     
        <style type="text/css">
#productDescription {
    color: #333333;
    word-wrap: break-word;
    font-size: small;
